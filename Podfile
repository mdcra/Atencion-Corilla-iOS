# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Atencion Corilla iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Atencion Corilla iOS
  pod 'Firebase/Core'
  pod 'FirebaseUI'
  pod 'Firebase/Messaging'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'GoogleMaps'
  pod 'GooglePlaces'

  target 'Atencion Corilla iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Atencion Corilla iOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
